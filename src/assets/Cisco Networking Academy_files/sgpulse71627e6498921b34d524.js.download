:host{
    width: 100%;  
}

.flipcard::after{
    content: "";
    height: 0;
    display: block;
    clear: both;
}

.flipcard__widget{
    margin: 0 -1%;
    display: flex;
    flex-wrap: wrap;
}

:host([device="small"]) .flipcard__widget{
    margin: 0%;
}

:host([dir="rtl"]) .flipcard__main{ 
    float: right;
}

.flipcard_fullwidth .flipcard__main{
    width:100%;
}

:host([device="small"]) .flipcard__main{
    width:100%;
    margin: 2% 0;
}

.flipcard_fullwidth .flipcard__item-front{
    background-color: rgb(251 251 251);
}

.flipcard__item-front:hover{
    box-shadow: var(--shadow-1);
}

.flipcard__item-back-inner{
    padding: calc(var(--padding) / 2);
    overflow: auto;
    height: 100%
}

:host([dir="rtl"]) .flipcard__item-back{
    text-align: right;
}

.flipcard__item-back:hover{
    box-shadow: var(--shadow-1);
    border: 1px solid rgb(var(--item-border-color-hover)); 
}

.flipcard__item .flip-icon {
    position: absolute;
    bottom: 10px;
    right: 10px;
    font-size: .9rem;
    font-style: italic;
    color: var(--neutral-1);
    display: none;
}

material-icon{
    display: flex;
