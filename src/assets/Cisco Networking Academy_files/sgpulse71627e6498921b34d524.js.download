//     border: var(--border-width) solid rgb(var(--item-border-color));
//     background-color: rgb(var(--item-color));
//     cursor: auto;
// }

input[type="range"] {
    -webkit-appearance: none;
    appearance: none;
    cursor: pointer;
    z-index: 1;
  }

.slider__item-input{
    background-color: rgb(var(--background-color-2));
    height: .375rem;
    border-radius: .375rem;
    background-image: linear-gradient(90deg, rgb(var(--background-color-5)) 50%, rgb(var(--item-color-selected)) 50%);
    background-size: 200%;
}

.slider__item-input::-webkit-slider-thumb {
    -webkit-appearance: none;
    appearance: none;
    width: 2rem;
    height: 2rem;
    // background: rgb(var(--background-color-0));
    background: rgb(var(--background-color-5));
    border: 16px solid rgba(var(--background-color-3),0.5);
    cursor: pointer;
    box-shadow: 0 2px 5px rgba(0,0,0,0.3);
    border-radius: 50%;
  }
  
  .slider__item-input::-moz-range-thumb {
    width: 2rem;
    height: 2rem;
    // background: rgb(var(--background-color-0));
    background: rgb(var(--background-color-1));
    border: 0px solid rgba(var(--background-color-3),0.5);
    cursor: pointer;
    box-shadow: 0 2px 5px rgba(0,0,0,0.3);
    border-radius: 50%;
  }

  .slider__item {
    margin-top: calc((var(--margin-top)) - 2.5em);
    margin-bottom: calc((var(--margin-bottom)) - 2em);
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .slider__scale-notch{
    background-color: rgb(var(--item-color-inverted));
    opacity: 0.3;
    height: 0.6em;
    width: 0.07em;
  }

  .slider__scale-container{
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: calc((var(--margin)) - 2.75em);
    margin-top: calc((var(--margin-top)) - 3.5em);
    position: relative;
  }

  .slider__scale-notch-container{
    position: absolute;
    background-color: rgb(var(--item-color-inverted));
    opacity: 0.3;
    height: 0.07em;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-evenly;
  }

  .slider__scale-notch-container .slider__scale-notch{
    opacity: 1;
  } 

  .component__widget.is-disabled .slider__number{
    pointer-events: none;
  }
  .component__widget.is-disabled .slider__item-input::-webkit-slider-thumb {
    -webkit-appearance: none;
    appearance: none;
    cursor: auto;
  }
  .component__widget.is-disabled .slider__item-input::-moz-range-thumb {
    cursor: auto;
  }
  .component__widget.is-disabled .slider__item{
    opacity: 0.5;
  }

  input[type="range"]:disabled {
    -webkit-appearance: none;
    appearance: none;
    cursor: auto;
  }

  .slider__label-end-inner{
    text-align: right;
  }

  .slider__highlighter{
    position: absolute;
    height: 2.5rem;
    width: 2.5rem;
   border: var(--border-width) solid rgb(var(--item-border-color));
    background-color: rgb(var(--item-color));
    cursor: auto;
    border-radius: 50%;
    left: 0px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .slider__widget.show-user-answer.is-correct .slider__highlighter {
    background-color: #1c8e0d;
    color: #fff;
  }

  .dark-theme .slider .slider__inner .slider__widget .slider__number-container .slider__highlighter {
    background-color: #212328 !important;
    color: #ffffff !important;
    border: 1px solid #3c4148 !important;
  }

  .is-submitted .slider__number.is_incorrect {
    background-color: #FF0000;
    color: rgb(var(--white-color));
    border: none;
  }

  .slider__widget.show-user-answer.is-incorrect .slider__highlighter {
    background-color: red;
    color: #fff;
  }
  
  .slider__widget.show-correct-answer.is-incorrect .slider__highlighter {
    background-color: #1c8e0d;
    color: #fff;
    border: none;
}
  .multipleCorrectAnswer {
    background-color: #1c8e0d;
    color: #fff;
  }
.showScale {
   visibility: hidden;
 }
.showScaleNumber {
  visibility: hidden;
  }

