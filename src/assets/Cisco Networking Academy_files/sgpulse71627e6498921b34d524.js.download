.tabs__content_image .tabs__content-item .tabs__content-item-inner {
  .tabs__content-item-body {
    display: inline-block;
    width: 48%;
 }
   .tabs__content-item-graphic{
    display: inline-block;
    width: 48%;
    float: right;
    }
    &.stacked {
      .tabs__content-item-body {
          width: 100%;
       }
       .tabs__content-item-graphic{
         width: 100%;
         float: none
        }
    }
 }
  .tabs__content_image .tabs__content-item .tabs__content-item-inner .tabs__content-item-graphic{
    display: inline-block;
    width: 48%;
    float: right;
  }
