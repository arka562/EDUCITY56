    float: right;
}

material-icon.front-icon{
    position: absolute;
    bottom: 6px;
    right: 13px;
    color: rgb(182 183 184);
    transform: none;
}

.flipcard__item-back-title{
    font-size: calc(var(--font-size-base) + 0.05em);
}

.flipcard__item-front:hover{
    border: 1px solid rgb(var(--item-color-hover)) !important;
}

.flipcard__item-face img:hover {
    filter: grayscale(1);
