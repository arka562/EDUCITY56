:host([dir="rtl"]) .hotgraphic-popup__item-image-container{
    margin: auto;
    margin-right: 1rem;
}
.hotgraphic-popup__item-image-container img{
    display: block;
    min-width: 100%;
}

.hotgraphic-popup__toolbar{
    background-color: rgb(var(--background-color-2));
    color: rgb(var(--font-color-1));
    height: 3.75em;
    overflow: hidden;
    z-index: 1;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-between;
    width: 100%;
}

.hotgraphic-popup__nav button {
    background-color: rgb(var(--background-color-2));
    color: rgb(var(--font-color-1));
    left: 0;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 0;
    z-index: 1;
    text-align: center;
    border-radius: 0%;
    height: 3.75em;
    line-height: 3.75em;
    width: 3.75em;
}
.hotgraphic-popup__nav button.next {
    left: 3.875em;
}
.hotgraphic-popup__close {
    background-color: rgb(var(--background-color-2));
    color: rgb(var(--font-color-1));
    left: auto;
    right: 0;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 0;
    z-index: 1;
    text-align: center;
    border-radius: 0%;
}

:host([dir="rtl"]) .hotgraphic-popup__close{
    left: 0;
    right: auto;
}

:host([dir="rtl"]) .hotgraphic-popup__nav button.next{
    right: 3.875em;
}

:host([dir="rtl"]) .hotgraphic-popup__nav button{
    left: auto;
    right: 0;
    transform: rotateZ(180deg);
}

.hotgraphic-popup__close:hover{
    background-color: rgb(var(--background-color-5)) !important;
}

.hotgraphic-popup__inner:focus{
    outline:none;
}
.hotgraphic-popup__count:focus{
    outline:none;
}


.hotgraphic-close-btn{
    color: rgb(var(--font-color-1));
    height: 3.75em;
    line-height: 3.75em;
    width: 3.75em;
}


.hotgraphic-popup__inner .hotgraphic-popup__item-content .hotgraphic-popup__item-content-inner .hotgraphic-popup__item-title {
    font-size: 1.4em;
    font-weight: normal;
    font-family: "Open Sans",arial,sans-serif;
    letter-spacing: -1px;
    margin-bottom: 0.8em;
    line-height: 1.2;
}
.hotgraphic-popup__inner .hotgraphic-popup__item-content .hotgraphic-popup__item-body {
    font-size: 1em;
    font-weight: normal;
    letter-spacing: normal;
    font-family: "Open Sans",arial,sans-serif;
}

:host([dir="rtl"]) .hotgraphic-popup__inner {
    text-align: right;
}

.hotgraphic-popup__count {
    left: 0;
    letter-spacing: -0.125em;
    margin: 0;
    padding: 0;
    position: absolute;
    top: 0;
    width: 100% !important;
    text-align: center;
    height: 3em;
    line-height: 3em;
    font-size: 1.2375em;
    direction: ltr;
} 

.hotgraphic-popup__controls:hover{
    background-color: rgb(var(--background-color-5)) !important;
}

.hotgraphic-next-btn,
.hotgraphic-back-btn{
    color: rgb(var(--font-color-1));
    height: 3.75em;
    line-height: 3.75em;
    width: 3.75em;
    transform: scale(0.8);
}


.cancel-popup-close-icon{
    display:none;
}

.disabled{
    display:none;
}

