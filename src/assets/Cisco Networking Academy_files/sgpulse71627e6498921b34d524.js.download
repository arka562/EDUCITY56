.healthguage-container{
    position: fixed;
    top: 5em;
    right: 1em;
    width: 5em;
    height: 5em;
    z-index: 2;
    margin: 0;
}

:host([dir="rtl"]) .healthguage-container{
  left: 1em;
  right: auto;
}

.healthgauge-button {

    box-shadow: var(--shadow-1);
    background: transparent;
    border-radius: 50%;
    width: 5em;
    height: 5em;
    overflow: hidden;
    position:relative;
  }

  :host([dir="rtl"]) .healthgauge-button {
    left: 1em;
    right: inherit;
  }


  .healthgauge-button:after {
    border: .2em solid rgb(var(--background-color-5));
    clear: both;
    content: "";
    display: table;
    border-radius: 50%;
    height: 100%;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: -1000;
  }

  .healthgauge-button .healthgauge-progress {
    overflow: hidden;
    border-radius: 50%;
    height: 100%;
    width: 100%;
    display: inline-block;
  }

  .healthgauge-button .healthgauge-progress #svg {
    transform: rotate(-90deg);
    border-radius: 50%;
    position: absolute;
    top: 0px;
    left: 0px;
    font-size: 8em;
  }

  .healthgauge-button .healthgauge-progress #svg circle {
    stroke-dashoffset: 0;
    transition: stroke-dashoffset 1s linear;
    stroke: rgb(var(--background-color-5));
    stroke-width: 1.6em;
  }

  .healthgauge-button .healthgauge-progress #svg #bar {
    stroke: rgb(var(--primary-color));
    // stroke-dashoffset: 424.115px;
  }


  .healthgauge-button .healthgauge-progress img {
    display: none;
  }


  .healthgauge-button .healthgauge-progress__icon material-icon{
    transform: scale(1.2);
  }


  .healthgauge-button .healthgauge-progress__icon {
    background: rgb(var(--background-color-0)) !important;
    border-radius: 50%;
    bottom: 10%;
    display: block;
    font-size: calc(var(--font-size-base) * 2);
    left: 10%;
    margin-right: 0;
    position: absolute;
    right: 10%;
    text-align: center;
    top: 10%;
    height: 80%;
    width: 80%;
    z-index: 10;
    display: flex;
    align-items: center;
    justify-content: center;
  }


  .healthgauge-button .healthgauge-progress__icon:hover {
    transform: scale(1.1);
    transform-origin: 50% 50%;
    transition: transform .3s ease-in-out;
  }

  .healthgauge-popup {
    width: 100%;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99999;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .healthgauge-popup .healthgauge-popup__overlay {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    // background-color: rgba(var(--neutral-1), 0.5);
    background-color: rgba(42,42,42,0.8);
  }


  .healthgauge-popup .healthgauge-popup__inner {
    background-color: rgb(var(--background-color-4));
    z-index: 99;
    padding: 3em 1.25em 3em 1.25em;
    position: fixed;
    text-align: center;
    flex-direction: column;
    max-width: 32.5em;
    max-height: 100%;
    overflow-y: auto;
    width: 100%;
    min-height: 40vh;
    max-height: 80vh
    border-radius: var(--border-radius);
    overflow: hidden;
  }

  :host([device="small"]) .healthgauge-popup .healthgauge-popup__inner{
    height: 100vh;
    max-height: 100vh;
  }

  .healthgauge-popup .healthgauge-popup__inner .healthgauge-popup__close {
    background-color: rgb(var(--background-color-4));
    color: rgb(var(--neutral-1));
    height: auto;
    margin: 0;
    padding: .75em;
    transition: background-color .3s linear;
    width: auto;
    z-index: 10;
    position: absolute;
    right: 0;
    top: 0;
  }

  .healthgauge-popup .healthgauge-popup__inner .healthgauge-popup__close:hover {
    background-color: rgb(var(--item-color));
  }

  .healthgauge-popup .healthgauge-popup__inner .healthgauge-progress {
    display: inline-block;
    margin: 0;
  }


  .healthgauge-popup .healthgauge-popup__inner .healthgauge-progress .healthgauge-progress__bar {
    height: 20vh;
    width: auto;
  }

  .healthgauge-popup .healthgauge-popup__inner .healthgauge-progress img {
    height: inherit;
    max-width: 25em;
    min-width: 0;
    vertical-align: bottom;
  }

  .healthgauge-popup .healthgauge-popup__inner .healthgauge-labels {
    display: none;
  }

  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components{
    height: 100%;
  }

  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner {
    border-radius: var(--border-radius);
    border: var(--border-width) solid rgb(var(--item-border-color));
    font-size: var(--font-size-base);
    line-height: 1.1;
    margin: .625rem 0;
    overflow-y: auto;
    max-height: 40vh;
  }

  :host([device="small"]) .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner {
    max-height: 62vh;
  }
  
  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__description-inner {
    color: rgb(var(--neutral-1));
    font-size: var(--font-size-base);
    margin: 1em 0;
    text-align: start;
    padding: 0.625em;
  }
  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__list ol {
    padding: .625em;
    margin: 0;
  }
  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__list ol li {
    margin: 0 0 .625em 1.125em;
  }
  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__list ol li::marker {
    color: transparent;
  }
  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__list ol .healthgauge-components__item-link {
    cursor: pointer;
    display: flex;
    justify-content: space-between;
    margin-bottom: .5em;
    color: rgb(var(--neutral-1));
    font-size: 1em;
    line-height: 1.75em;
  }
  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__list ol .healthgauge-components__item-link .healthgauge-components__item-title {
    flex-grow: 1;
    padding-right: 5%;
    color: rgb(var(--neutral-1));
    text-align: left;
  }
  .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__list ol .healthgauge-components__item-link .healthgauge-components__item-status {
    color: rgb(var(--primary-color));
    text-align: right;
    width: 33%;
  }

  :host([device="small"]) .healthgauge-popup .healthgauge-popup__inner .healthgauge-components .healthgauge-components__inner .healthgauge-components__list ol .healthgauge-components__item-link .healthgauge-components__item-status {
    width: 65%;
  }

  :host([dir="rtl"]) .healthgauge-components__item-status{
    text-align: left !important;
  }
  
  :host([dir="rtl"]) .healthgauge-components__item-title{
    text-align: right !important;
  }

  .healthgauge-components__item-status .status{
    padding: 0 0.3em;
  }
  :host([dir="rtl"]) .healthgauge-popup .healthgauge-popup__inner .healthgauge-popup__close{
    left: 0;
    right: initial;
  }

