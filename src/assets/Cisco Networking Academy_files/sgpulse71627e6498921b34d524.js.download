:host([device="medium"]) .slider__number, :host([device="medium"]) .slider__highlighter{
  height: 2rem;
    width: 2rem;
}

:host([device="small"]) .slider__number, :host([device="small"]) .slider__highlighter{
  height: 1.6rem;
    width: 1.6rem;
    font-size: var(--font-size-sm);
}

:host([device="medium"]) .slider__number-container{
  margin: 0px 4px;
}

:host([device="small"]) .slider__number-container{
  margin: 0px 8px;
}

.slider__number-container{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0;
    position: relative;
}

.slider__item-input{
    width: calc(100% - 0.5rem);
}

.slider__label-container{
    display: flex;
    justify-content: space-between;
    margin-bottom: calc((var(--margin-bottom)) - 3.7em);
}

.slider__number-model-range, .slider__number-answer, .slider__number-selection{
    display: none;
