.flipcard__main{
    position:relative;
    width:48%;
    float:left;
    margin: 0.6em 1%;
    box-sizing:border-box;
    cursor: pointer;
    perspective: 600px;
}

.flipcard__item {
    position: relative;
    width: 100%;
    transform-style: preserve-3d;
    transform-origin: center right;
    transition:all 1s ease;
    background-color: rgb(var(--item-color));
    border-radius: var(--border-radius);
}

.flipcard__item.flip{
    transform: translateX(-100%) rotateY(-180deg);
} 

.flipcard__item-face{
    padding: 0.4em;
    position:relative;
    width:100%;
    color: white;
    text-align: center;
    backface-visibility: hidden;
    border-radius: var(--border-radius);
    background-color: rgb(var(--item-color));
    border: 1px solid rgb(var(--item-border-color));
}

.flipcard__item-back{
    position:absolute;
    transform: rotateY(180deg);
    top:0px;
    left:0px;
    width:100%;
    height:100%;
    background-color: rgb(var(--item-color));
    border: 1px solid rgb(var(--item-border-color));
    border-radius: var(--border-radius);
    box-sizing:border-box;
    text-align: left;
    line-height: 1.5;
    color: rgb(var(--item-color-inverted));
}
