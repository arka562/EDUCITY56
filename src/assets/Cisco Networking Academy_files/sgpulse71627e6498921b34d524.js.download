        color: var(--font-color-1);
        margin: 4px;
        text-align: center;
        word-spacing: -2px;
        border: 1px solid #6abf4b;
        border-radius: 4px;
        width: 28px;
        height: 28px;
        box-sizing: border-box;
        font-size: 12px;
        float: initial;
        background-color: #549f3820;
        font-weight: 400;
        line-height: 29px;
    }
    
    .review-container .attempted-blocks-wrapper .block-button:hover {
        background-color: rgba(var(--primary-color), 0.5) !important;
        border: 1px solid rgba(var(--primary-color), 0.5) !important;
        color: rgb(var(--neutral-1)) !important;
    }

    .review-container .attempted-blocks-wrapper .highlight:hover {
        background-color: #e2231a50 !important;
        border: 1px solid #E2231A !important;
    }

    .review-container .attempted-blocks-wrapper .active-block {
        background-color: rgb(var(--neutral-2));
    }

    .review-container .attempted-blocks-wrapper .last-block {
        background-color: rgb(var(--primary-color)) !important;
        font-size: 12px;
        color: #000;
    }

