    .component-status.component-status-pending material-icon{
        // color: rgb(var(--background-color-5));
        color: rgb(219, 219, 219);
    }

    .component-status.component-status-inProgress{
        background: rgb(255, 255, 255);
    }

    .component-status.component-status-inProgress material-icon{
        color: rgb(var(--primary-color));
    }

    .component-status.component-status-complete{
        background-color: rgb(var(--primary-color));
    }

    .component-status.component-status-complete material-icon{
        color: rgb(var(--primary-color-inverted));
    }
