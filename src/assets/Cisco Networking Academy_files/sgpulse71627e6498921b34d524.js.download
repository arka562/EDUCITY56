        height: 2.625rem;
        border-radius: 50%;
        color: rgb(var(--white-color));
        box-shadow: 0 0.125rem 0.375rem rgba(0,0,0,0.4);
        line-height: 1.7;
    }

    :host([dir="rtl"]) .btn__marking{
        right: initial;
        top: 50%;
        left: 1em;
    }


    .review-button-container {
        width: 100%;
        display: flex;
        justify-content: center;
    }

    .review-assessment {
        background-color: transparent;
        color: #3d692b;
        text-decoration: underline;
        padding-bottom: 3rem;
    }

    .submit__anyway-checkbox-container{
        display: flex;
        align-items: center;
        gap: 5px;
        user-select:none;
        background: transparent;
    }
    :host([device="small"]) .submit__anyway-checkbox-container{ 
        gap:2px;
    }
    .submit__anyway-checkbox-container #submit-anyway-checkbox-label{
        color: rgb(var(--item-color-inverted));
    }
    .submit__anyway-wrapper{
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 1em;
        cursor:pointer;
    }
    :host([device="small"]) .submit__anyway-wrapper{
      padding: 0rem 0.9375rem;
    }
    .submit__anyway-checkbox-icon{
        color: rgb(var(--item-color-inverted));
    }

    .submit__anyway-checkbox-icon[icon="check_box"]{
       color: rgb(var(--primary-color));
    }

      .btn__action.is-disabled, .btn__action[disabled]{
        background-color: rgb(var(--disable-button-color));
        color: rgb(var(--font-color-2));
        border: var(--border-width) solid rgb(var(--disable-button-color));
        opacity: .5;
        pointer-events: none;
    }
