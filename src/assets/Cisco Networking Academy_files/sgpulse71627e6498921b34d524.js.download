    text-align: right;
  }
  .tabs__content-inner{
    transform: translateY(20px);
    opacity: 0;
}


.delayed-transform{
      transform: translateY(0px);
      opacity: 1;
      transition: transform 0.5s linear, opacity 0.5s linear;
  }
  
  .tabs__item-image-container .iframe-container .webcomponent{
